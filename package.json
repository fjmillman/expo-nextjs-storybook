{
  "name": "expo-nextjs-storybook",
  "version": "1.0.0",
  "private": true,
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "expo:start": "expo start",
    "expo:android": "expo start --android",
    "expo:ios": "expo start --ios",
    "expo:eject": "expo eject",
    "next:dev": "next dev",
    "next:web": "next",
    "next:build": "next build",
    "next:eject": "next-expo customize",
    "start-storybook": "NODE_ENV=storybook start-storybook -p 6006",
    "build-storybook": "NODE_ENV=storybook build-storybook",
    "test-storybook": "test-storybook",
    "test-storybook:ci": "concurrently -k -s first -n \"SB,TEST\" -c \"magenta,blue\" \"npm run build-storybook --quiet && npx http-server storybook-static --port 6006 --silent\" \"wait-on tcp:6006 && npm run test-storybook\"",
    "chromatic": "chromatic --skip 'dependabot/**' --exit-zero-on-changes --exit-once-uploaded",
    "storybook:native:stories": "sb-rn-get-stories --config-path .storybook/react-native",
    "storybook:native:watch": "sb-rn-watcher --config-path .storybook/react-native",
    "storybook:native:start": "npm run storybook:native:stories && IS_STORYBOOK=true npm run expo:start",
    "storybook:native:ios": "npm run storybook:native:stories && IS_STORYBOOK=true npm run expo:ios",
    "storybook:native:android": "npm run storybook:native:stories && IS_STORYBOOK=true npm run expo:android",
    "prepare": "husky install",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "test": "jest --watch --coverage=false --changedSince=origin/main",
    "test:debug": "jest -o --watch --coverage=false",
    "test:final": "jest"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "eslint --fix"
  },
  "dependencies": {
    "expo": "^44.0.0",
    "expo-constants": "~13.0.1",
    "next": "^12.1.0",
    "next-compose-plugins": "^2.2.1",
    "next-fonts": "^1.5.1",
    "next-images": "^1.8.4",
    "next-transpile-modules": "^9.0.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-native": "0.64.3",
    "react-native-web": "0.17.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@expo/next-adapter": "^3.1.20",
    "@react-native-async-storage/async-storage": "^1.15.17",
    "@react-native-community/datetimepicker": "^4.0.0",
    "@react-native-community/slider": "^4.1.12",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-controls": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-interactions": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/addon-ondevice-actions": "^6.0.1-beta.3",
    "@storybook/addon-ondevice-backgrounds": "^6.0.1-beta.3",
    "@storybook/addon-ondevice-controls": "^6.0.1-beta.3",
    "@storybook/addon-ondevice-notes": "^6.0.1-beta.3",
    "@storybook/addon-react-native-web": "0.0.18",
    "@storybook/builder-webpack5": "^6.4.19",
    "@storybook/jest": "0.0.9",
    "@storybook/manager-webpack5": "^6.4.19",
    "@storybook/react": "^6.4.19",
    "@storybook/react-native": "^6.0.1-beta.3",
    "@storybook/test-runner": "0.0.4",
    "@storybook/testing-library": "0.0.9",
    "@types/jest": "^26.0.14",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/react-native": "^0.66.16",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "babel-loader": "^8.2.3",
    "babel-preset-expo": "^9.0.2",
    "chromatic": "^6.5.1",
    "concurrently": "^7.0.0",
    "core-js": "^3.21.1",
    "eslint": "^8.10.0",
    "eslint-config-next": "^12.1.0",
    "eslint-config-universe": "^10.0.0",
    "eslint-plugin-storybook": "^0.5.7",
    "http-server": "^14.1.0",
    "husky": "^7.0.0",
    "jest": "^26.6.3",
    "jest-expo": "^44.0.1",
    "lint-staged": "^12.3.4",
    "prettier": "^2.5.1",
    "react-test-renderer": "^17.0.2",
    "ts-jest": "^26.5.6",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5",
    "wait-on": "^6.0.1",
    "webpack": "^5.69.1"
  }
}
