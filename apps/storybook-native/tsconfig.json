{
  "extends": "@/tsconfig/base.json",
  "compilerOptions": {
    "baseUrl": ".",
    "jsx": "react-native",
    "types": ["node", "../../tooling/eslint/types"],
    "checkJs": false,
    "moduleSuffixes": [".ios", ".android", ".native", ""]
  },
  "include": [
    "src",
    "*.ts",
    "*.js",
    ".expo/types/**/*.ts",
    ".storybook/main.ts",
    ".storybook/preview.tsx"
  ],
  "exclude": ["node_modules", ".storybook/storybook.requires.ts"]
}
