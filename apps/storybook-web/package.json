{
  "name": "storybook-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "storybook build",
    "clean": "git clean -xdf .cache .next .turbo node_modules storybook-static",
    "deploy": "chromatic --skip 'dependabot/**' --exit-zero-on-changes --exit-once-uploaded",
    "dev": "storybook dev -p 6006",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "test": "test-storybook --url http://localhost:6006",
    "test:ci": "pnpm dlx start-server-and-test \"pnpm dlx http-server storybook-static --port 6006 --silent\" 6006 \"pnpm test\"",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@/ui": "workspace:*",
    "@expo/next-adapter": "6.0.0",
    "@storybook/addon-a11y": "catalog:storybook8",
    "@storybook/addon-essentials": "catalog:storybook8",
    "@storybook/addon-interactions": "catalog:storybook8",
    "@storybook/addon-react-native-web": "catalog:storybook8",
    "@storybook/nextjs": "catalog:storybook8",
    "@storybook/react": "catalog:storybook8",
    "@storybook/react-native": "catalog:storybook8",
    "@storybook/react-webpack5": "catalog:storybook8",
    "@storybook/test": "catalog:storybook8",
    "@storybook/test-runner": "catalog:storybook8",
    "chromatic": "catalog:storybook8",
    "nativewind": "catalog:",
    "next": "14.2.23",
    "raf": "^3.4.1",
    "react": "catalog:react18",
    "react-dom": "catalog:react18",
    "react-native": "~0.76.6",
    "setimmediate": "^1.0.5",
    "storybook": "catalog:storybook8"
  },
  "devDependencies": {
    "@/eslint-config": "workspace:*",
    "@/prettier-config": "workspace:*",
    "@/tailwind-config": "workspace:*",
    "@/tsconfig": "workspace:*",
    "@babel/core": "^7.26.8",
    "@jest/globals": "catalog:jest29",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "~16.2.0",
    "@types/babel__core": "~7.20.5",
    "@types/jest": "catalog:jest29",
    "@types/node": "20.12.7",
    "@types/raf": "~3.4.3",
    "@types/react": "catalog:react18",
    "@types/react-dom": "catalog:react18",
    "@types/react-test-renderer": "~18.3.1",
    "@types/setimmediate": "~1.0.4",
    "autoprefixer": "^10.4.19",
    "babel-loader": "^9.2.1",
    "babel-plugin-react-native-web": "^0.19.13",
    "eslint": "catalog:",
    "jest": "catalog:jest29",
    "prettier": "catalog:",
    "react-test-renderer": "18.3.1",
    "tailwindcss": "catalog:",
    "ts-jest": "catalog:jest29",
    "typescript": "catalog:"
  }
}
