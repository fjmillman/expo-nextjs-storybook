{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {},
    "test": {},
    "test:ci": {
      "outputs": ["coverage/**"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "typecheck": {
      "dependsOn": ["^typecheck"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false
    }
  }
}
